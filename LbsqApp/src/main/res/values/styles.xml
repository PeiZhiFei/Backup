    <style name="LoginRegister" parent="@android:style/Widget">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="WrapContent" parent="@android:style/Widget">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="width_match_parent_wihte" parent="@android:style/Widget">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="width_match_parent" parent="@android:style/Widget">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/backgroundgray</item>
    </style>

    <style name="width_height_match_parent" parent="@android:style/Widget">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="Transparent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
    </style>

    <style name="judgeRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bg</item>
        <item name="android:minHeight">13dp</item>
        <item name="android:maxHeight">13dp</item>
    </style>

    <style name="register_edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/darkgray</item>
        <item name="android:textColorHint">@color/darkgray2</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">20dp</item>
    </style>

    <style name="include_order_info">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="include_order_info_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:background">@null</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textColorHint">#999999</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="include_order_info_bg">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ads_image">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">110dp</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="buynow_include_left">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="buynow_include_right">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textColorHint">#999999</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="youhuiquan">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/darkgray2</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:drawableLeft">@drawable/shape_circle_green</item>
    </style>

</resources>